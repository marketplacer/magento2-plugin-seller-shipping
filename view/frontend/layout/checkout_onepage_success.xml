<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="checkout.success.print.button" remove="true"/>
        <referenceContainer name="content">
            <referenceBlock name="checkout.success" remove="true"/>
            <block name="checkout.success.marketplacer" template="Marketplacer_SellerShipping::checkout/success.phtml" cacheable="false">
                <arguments>
                    <argument name="view_model" xsi:type="object">Marketplacer\SellerShipping\ViewModel\Checkout\Onepage\Success\SuccessAdditionalInfo</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
